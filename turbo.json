{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_VERCEL_URL",
        "NODE_ENV",
        "DATABASE_URL",
        "BETTER_AUTH_SECRET"
      ],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    }
  }
}
